{
  "name": "maven-dependency-explorer",
  "displayName": "Maven dependency explorer",
  "description": "Explore depency of java maven project",
  "repository": "https://github.com/Dhruvkumarshah/maven-dependency-explorer.git",
  "version": "1.0.14",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:maven-dependency-explorer.exploer"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "maven-dependency-explorer.exploer",
        "title": "Maven dependency explorer"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceFilename == pom.xml",
          "command": "maven-dependency-explorer.exploer",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^10.2.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "graphlib-dot": "^0.6.4"
  },
  "publisher": "dhruv"
}
